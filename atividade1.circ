  <vhdl name="TwoComplementSubtractor_vhdl">LIBRARY ieee;
USE ieee.std_logic_1164.all;

ENTITY TwoComplementSubtractor_vhdl IS
  PORT (
    A     : IN  std_logic_vector(3 downto 0); 
    B     : IN  std_logic_vector(3 downto 0); 
    S     : OUT std_logic_vector(4 downto 0)
                   
  );
END TwoComplementSubtractor_vhdl;

ARCHITECTURE TypeArchitecture OF TwoComplementSubtractor_vhdl IS
  SIGNAL CARRY : std_logic_vector(3 downto 0);
BEGIN
  CARRY(0) &lt;= '1';
  
  S(0) &lt;= A(0) XOR NOT B(0) XOR CARRY(0);
  CARRY(1) &lt;= (A(0) AND NOT B(0)) OR (A(0) AND CARRY(0)) OR (NOT B(0) AND CARRY(0));

  S(1) &lt;= A(1) XOR NOT B(1) XOR CARRY(1);
  CARRY(2) &lt;= (A(1) AND NOT B(1)) OR (A(1) AND CARRY(1)) OR (NOT B(1) AND CARRY(1));

  S(2) &lt;= A(2) XOR NOT B(2) XOR CARRY(2);
  CARRY(3) &lt;= (A(2) AND NOT B(2)) OR (A(2) AND CARRY(2)) OR (NOT B(2) AND CARRY(2));

  S(3) &lt;= A(3) XOR NOT B(3) XOR CARRY(3);
  S(4) &lt;= (A(3) AND NOT B(3)) OR (A(3) AND CARRY(3)) OR (NOT B(3) AND CARRY(3));
  
END TypeArchitecture;
