  <vhdl name="HalfSubtractor_vhdl">LIBRARY ieee;
USE ieee.std_logic_1164.all;

ENTITY HalfSubtractor_vhdl IS
  PORT (
    A      : IN  std_logic;
    B      : IN  std_logic;
    S      : OUT std_logic;
    Cout   : OUT std_logic
  );
END HalfSubtractor_vhdl;

ARCHITECTURE TypeArchitecture OF HalfSubtractor_vhdl IS
BEGIN
  S &lt;= A XOR B;
  Cout &lt;= NOT A AND B;
END TypeArchitecture;
</vhdl>
  <vhdl name="HalfAdder_vhdl">LIBRARY ieee;
USE ieee.std_logic_1164.all;

ENTITY HalfAdder_vhdl IS
  PORT (
    A      : IN  std_logic;
    B      : IN  std_logic;
    S      : OUT std_logic;
    Cout   : OUT std_logic
  );
END HalfAdder_vhdl;

ARCHITECTURE TypeArchitecture OF HalfAdder_vhdl IS
BEGIN
  S &lt;= A XOR B;
  Cout &lt;= A AND B;
END TypeArchitecture;
</vhdl>
  <vhdl name="FullSubtractor_vhdl">LIBRARY ieee;
USE ieee.std_logic_1164.all;

ENTITY FullSubtractor_vhdl IS
  PORT (
    A      : IN  std_logic;
    B      : IN  std_logic;
    Cin    : IN  std_logic;
    S      : OUT std_logic;
    Cout   : OUT std_logic
  );
END FullSubtractor_vhdl;

ARCHITECTURE TypeArchitecture OF FullSubtractor_vhdl IS
BEGIN
  S &lt;= (A XOR B) XOR Cin;
  Cout &lt;= (NOT A AND B) or (NOT A AND Cin) or (B AND Cin);
END TypeArchitecture;
</vhdl>
  <vhdl name="FullAdder_vhdl">LIBRARY ieee;
USE ieee.std_logic_1164.all;

ENTITY FullAdder_vhdl IS
  PORT (
    A      : IN  std_logic;
    B      : IN  std_logic;
    Cin    : IN  std_logic;
    S      : OUT std_logic;
    Cout   : OUT std_logic
  );
END FullAdder_vhdl;

ARCHITECTURE TypeArchitecture OF FullAdder_vhdl IS
BEGIN
  S &lt;= (A XOR B) XOR Cin;
  Cout &lt;= (A AND B) or (A AND Cin) or (B AND Cin);
END TypeArchitecture;
