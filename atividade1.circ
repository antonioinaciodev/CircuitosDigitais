  <vhdl name="HalfAdderSubtractor_vhdl">LIBRARY ieee;
USE ieee.std_logic_1164.all;

ENTITY HalfAdderSubtractor_vhdl IS
  PORT (
    A      : IN  std_logic;
    B      : IN  std_logic;
    Cin    : IN  std_logic;
    M      : IN  std_logic;
    S      : OUT std_logic;
    Cout   : OUT std_logic
  );
END HalfAdderSubtractor_vhdl;

ARCHITECTURE TypeArchitecture OF HalfAdderSubtractor_vhdl IS
BEGIN
  S &lt;= (A XOR B) XOR Cin;
  Cout &lt;= ((M XOR A) AND B) OR ((M XOR A) AND Cin) OR (B AND Cin);
END TypeArchitecture;
</vhdl>
  <vhdl name="Sutractor4bits_vhdl">LIBRARY ieee;
USE ieee.std_logic_1164.all;

ENTITY Sutractor4bits_vhdl IS
  PORT (
    A     : IN  std_logic_vector(3 downto 0); 
    B     : IN  std_logic_vector(3 downto 0); 
    S     : OUT std_logic_vector(4 downto 0)                    
  );
END Sutractor4bits_vhdl;

ARCHITECTURE TypeArchitecture OF Sutractor4bits_vhdl IS
  SIGNAL CARRY : std_logic_vector(3 downto 0);
BEGIN
  CARRY(0) &lt;= '0';

  S(0) &lt;= A(0) XOR B(0) XOR CARRY(0);
  CARRY(1) &lt;= (NOT A(0) AND B(0)) OR (NOT A(0) AND CARRY(0)) OR (B(0) AND CARRY(0));

  S(1) &lt;= A(1) XOR B(1) XOR CARRY(1);
  CARRY(2) &lt;= (NOT A(1) AND B(1)) OR (NOT A(1) AND CARRY(1)) OR (B(1) AND CARRY(1));

  S(2) &lt;= A(2) XOR B(2) XOR CARRY(2);
  CARRY(3) &lt;= (NOT A(2) AND B(2)) OR (NOT A(2) AND CARRY(2)) OR (B(0) AND CARRY(2));

  S(3) &lt;= A(3) XOR B(3) XOR CARRY(3);
  S(4) &lt;= (NOT A(3) AND B(3)) OR (NOT A(3) AND CARRY(3)) OR (B(3) AND CARRY(3));
  
END TypeArchitecture;
</vhdl>
